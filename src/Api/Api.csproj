<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <RootNamespace>Dodgeball.TrustServer.Api</RootNamespace>
        <Version>1.0.1</Version>
        <Title>Dodgeball.TrustServer</Title>
        <Description>The Dodgeball Server SDK for .NET</Description>
        <PackageId>Dodgeball.TrustServer</PackageId>
        <Authors>Andrew Schwartz, Patrick Teague</Authors>
        <Copyright>Copyright (c) Dodgeball Inc, 2023</Copyright>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<PackageReadmeFile>README.md</PackageReadmeFile>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Flurl.Http.Net45" Version="3.2.0" />
      <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="8.0.0-preview.2.23128.3" />
      <None Include="docs\README.md" Pack="true" PackagePath="\"/>
    </ItemGroup>

</Project>
